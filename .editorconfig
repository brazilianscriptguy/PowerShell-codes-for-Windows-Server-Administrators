# Top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

# Matches PS1, VBS, and HTA files
[*.{PS1,VBS,HTA}]
indent_style = space
indent_size = 4

# Matches TXT files
[*.TXT]
indent_style = space
indent_size = 2
